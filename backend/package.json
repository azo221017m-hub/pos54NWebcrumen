{
  "name": "pos54nwebcrumen-backend",
  "version": "2.5.B12",
  "description": "Backend API REST para sistema POS Crumen - Sistema de Punto de Venta y Comanda Digital",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:verify": "ts-node src/scripts/verifyDatabase.ts",
    "db:seed-user": "ts-node src/scripts/seedUsuario.ts",
    "db:update-poscrumen": "ts-node src/scripts/updatePoscrumenPassword.ts",
    "db:reset-login-attempts": "ts-node src/scripts/resetLoginAttempts.ts",
    "db:create-superuser": "ts-node src/scripts/createSuperuser.ts"
  },
  "keywords": [
    "pos",
    "api",
    "rest",
    "typescript",
    "express",
    "mysql"
  ],
  "author": "Crumen",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.6.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
